{
  "name": "Zabbix Agent 2 Home Assistant add-on",
  "version": "1.7.0",
  "slug": "zabbix-agent2",
  "description": "Uses zabbix-agent2 package from current alpine version, also includes PostgreSQL and MongoDB plug-ins. To access docker api it is necessary to disable protection mode",
  "url": "https://github.com/pschmitt/home-assistant-addons/tree/main/zabbix-agent2",
  "arch": [
    "aarch64",
    "amd64"
  ],
  "startup": "system",
  "boot": "auto",
  "host_ipc": true,
  "docker_api": true,
  "ports": {
    "10050/tcp": 10050
  },
  "image": "pschmitt/home-assistant-addon-{arch}-zabbix-agent2",
  "options": {
    "server": "zabbix-server.local",
    "serveractive": "zabbix-server.local",
    "hostname": "homeassistant"
  },
  "schema": {
    "server": "str",
    "serveractive": "str",
    "hostname": "str",
    "tlspskidentity": "str?",
    "tlspsksecret": "str?"
  }
}
