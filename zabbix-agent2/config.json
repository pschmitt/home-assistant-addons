{
  "name": "Zabbix Agent 2",
  "version": "1.8.2",
  "slug": "zabbix-agent2",
  "description": "Zabbix Network Monitoring Agent 2",
  "url": "https://github.com/pschmitt/home-assistant-addons/tree/main/zabbix-agent2",
  "arch": [
    "aarch64",
    "amd64"
  ],
  "startup": "system",
  "boot": "auto",
  "host_network": true,
  "host_ipc": true,
  "docker_api": true,
  "ports": {
    "10050/tcp": 10050
  },
  "image": "pschmitt/home-assistant-addon-{arch}-zabbix-agent2",
  "options": {
    "server": "zabbix-server.local",
    "serveractive": "zabbix-server.local",
    "hostname": "homeassistant"
  },
  "schema": {
    "server": "str",
    "serveractive": "str",
    "hostname": "str",
    "tlspskidentity": "str?",
    "tlspsksecret": "str?",
    "userparameter": ["str?"]
  }
}
