{
  "name": "Zabbix Agent Home Assistant add-on",
  "version": "1.6.0",
  "slug": "zabbix-agent",
  "description": "Uses zabbix-agent package from current alpine version",
  "url": "https://github.com/pschmitt/home-assistant-addons/tree/main/zabbix-agent",
  "arch": [
    "armhf",
    "armv7",
    "aarch64",
    "amd64",
    "i386"
  ],
  "startup": "system",
  "boot": "auto",
  "host_ipc": true,
  "ports": {
    "10050/tcp": 10050
  },
  "image": "pschmitt/home-assistant-addon-{arch}-zabbix-agent",
  "options": {
    "server": "zabbix-server.local",
    "serveractive": "zabbix-server.local",
    "hostname": "homeassistant"
  },
  "schema": {
    "server": "str",
    "serveractive": "str",
    "hostname": "str",
    "tlspskidentity": "str?",
    "tlspsksecret": "str?",
    "userparameter": "str?"
  }
}
