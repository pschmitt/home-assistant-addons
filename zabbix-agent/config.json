{
  "name": "zabbix-agent",
  "version": "1.4",
  "slug": "zabbix-agent",
  "description": "Zabbix agent",
  "arch": [
    "armhf",
    "armv7",
    "aarch64",
    "amd64",
    "i386"
  ],
  "startup": "system",
  "boot": "auto",
  "full_access": true,
  "host_ipc": true,
  "host_pid": true,
  "host_network": true,
  "docker_api": true,
  "ports": {
    "10050/tcp": 10050
  },
  "image": "pschmitt/home-assistant-addon-{arch}-zabbix-agent",
  "options": {
    "server": "zabbix-server.local",
    "serveractive": "zabbix-server.local",
    "hostname": "homeassistant"
  },
  "schema": {
    "server": "str",
    "serveractive": "str",
    "hostname": "str"
  }
}
